{
  "name": "websites",
  "version": "3.0.0",
  "description": "The code that powers all of my websites.",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf build && tsc",
    "up:d": "npm run imgen-config && sudo docker compose up --build --force-recreate -d",
    "up": "npm run imgen-config && sudo docker compose up --build --force-recreate",
    "down": "sudo docker compose down --volumes --remove-orphans",
    "up:dev": "npm run imgen-config && sudo docker compose up",
    "up:dev:build": "npm run imgen-config && sudo docker compose up --build",
    "dev": "npm run down && npm run up:dev",
    "dev:build": "npm run down && npm run up:dev:build",
    "update": "npm run down && docker image rm websites || true && git pull && git submodule update --init --recursive --remote && npm run up:d",
    "imgen-config": "cp src/config/json/imgen.json src/imgen/config.json",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OwOWebsites/Primary.git"
  },
