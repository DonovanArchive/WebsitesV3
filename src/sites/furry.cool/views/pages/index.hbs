	<style>
		body {
			background-color: #2C2F33;
		}

		.hide {
			color: #2C2F33;
		}

		h1,
		h2 {
			color: #0FF;
		}

		div#buttons button.stop {
			position: fixed;
			left: 5px;
			bottom: 5px;
		}

		div#buttons button.start {
			position: fixed;
			right: 5px;
			bottom: 5px;
		}

		button.hidden {
			display: none;
		}
	</style>
	<script>
		window.active = true;
		const colors = ["FFF", "0FF", "00F", "F00", "F0F", "0F0", "d511b5", "00b41c", "e003dd", "ce0380"],
			used = [];

		function gen() {
			const v = colors[Math.floor(Math.random() * colors.length)];
			if (used.includes(v)) return gen();
			else used.push(v);
			return v;
		}

		window.i = setInterval(() => {
			used.splice(0, 10);
			if (!window.active) return;
			const elements = [
				...Array.from(document.querySelectorAll(".random")),
				...Array.from(document.querySelectorAll("a"))
			];

			for (const e of elements) {
				const c = gen();
				e.style.color = `#${c}`;
			}
		}, 1e3);
	</script>
